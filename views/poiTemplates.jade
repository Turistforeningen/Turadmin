script(type="text/template" id="poiPopupTemplate").
    <strong class="poi-navn"><%= navn %></strong><br>
    <p class="popup-edit btn btn-default">Endre</p>
    <p class="popup-delete btn btn-default">Slett</p>

script(type="text/template" id="poiTemplate").
    <div class="panel panel-default">
        <div class="panel-heading">
        <h4 class="panel-title">
                <a id="poiHeader" data-toggle="collapse" data-parent="#route-pois-accordion" href="#route-pois-accordion-<%= cid %>">Sted uten navn</a>
            </h4>
        </div>
        <div id="route-pois-accordion-<%= cid %>" class="panel-collapse collapse in">
            <div class="panel-body">
                <div class="row">
                    <div class="col-lg-6 col-md-6 col-sm-6">
                        <h5>Om stedet</h5>
                        <form class="form-horizontal" role="form">
                            <div class="form-group">
                                <label for="inputEmail3" class="col-sm-4 control-label">Navn</label>
                                <div class="col-sm-8">
                                    <input name="navn" type="email" class="form-control" id="inputEmail3" placeholder="Navn på sted">
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="inputEmail3" class="col-sm-4 control-label">Kategori</label>
                                <div class="col-sm-8">
                                    <select name="kategori" class="form-control">
                                        <option value=0>Velg en</option></option>
                                        <option value="Hytte">Hytte</option>
                                        <option value="Fjelltopp">Fjelltopp</option>
                                        <option value="Gapahuk">Gapahuk</option>
                                        <option value="Rasteplass">Rasteplass</option>
                                        <option value="Telplass">Telplass</option>
                                        <option value="Geocaching">Geocaching</option>
                                        <option value="Turpostkasse">Turpostkasse</option>
                                        <option value="Turorientering">Turorientering</option>
                                        <option value="Utsiktspunkt">Utsiktspunkt</option>
                                        <option value="Attraksjon">Attraksjon</option>
                                        <option value="Badeplass">Badeplass</option>
                                        <option value="Fiskeplass">Fiskeplass</option>
                                        <option value="Klatrefelt">Klatrefelt</option>
                                        <option value="Akebakke">Akebakke</option>
                                        <option value="Skitrekk">Skitrekk</option>
                                        <option value="Kitested">Kitested</option>
                                        <option value="Skøytevann">Skøytevann</option>
                                        <option value="Toalett">Toalett</option>
                                        <option value="Bro">Bro</option>
                                        <option value="Vadested">Vadested</option>
                                        <option value="Parkering">Parkering</option>
                                        <option value="Holdeplass">Holdeplass</option>
                                        <option value="Togstasjon">Togstasjon</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="inputEmail3" class="col-sm-4 control-label">Er også</label>
                                <div class="col-sm-8 flereStedKategorierSelectContainer">

                                </div>
                            </div>
                            <div class="form-group">
                                <label for="inputEmail3" class="col-sm-4 control-label">Beskrivelse</label>
                                <div class="col-sm-8">
                                    <textarea name="beskrivelse" class="form-control" rows="5"></textarea>
                                </div>
                            </div>
                        </form>
                    </div>
